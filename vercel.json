{
  "version": 2,
  "cleanUrls": true,
  "functions": {
    "api/send-report.js": {
      "memory": 3008,
      "maxDuration": 60,
      "includeFiles": "node_modules/@sparticuz/chromium/bin/**"
    }
  },
  "routes": [
    { "src": "^/$", "dest": "/index.html" },
    { "src": "^/index$", "dest": "/index.html" },
    { "src": "^/api/send-report$", "dest": "/api/send-report.js" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}
